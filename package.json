{
  "name": "angel-mat-ngxs",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "concurrently \"npm run start\" \"npm run serve:electron \"",
    "file": "ng build --base-href ./ && npm run main:electron && electron .",
    "build": "ng build --base-href ./ && npm run main:electron && electron-builder build",
    "package": "npm run build:lib && npm run npm:pack",
    "serve:electron": "wait-on tcp:4200 && npm run main:electron && electron . --serve",
    "main:electron": "tsc -p tsconfig.main.json",
    "build:lib": "ng build material",
    "npm:pack": "cd dist/material && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/logger-plugin": "^3.6.2-dev.master-eaa5d86",
    "@ngxs/store": "^3.6.2-dev.master-eaa5d86",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@types/core-js": "^2.5.4",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2",
    "wait-on": "^5.2.1"
  }
}
